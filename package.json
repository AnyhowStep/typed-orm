{
  "name": "typed-orm",
  "version": "0.9.2",
  "description": "A strongly typed MySQL ORM",
  "main": "dist/src/main/index.js",
  "types": "dist/src/main/index.d.ts",
  "scripts": {
    "test-sandbox": "ts-node ./src/main/sandbox/select-builder-v2",
    "test-all": "ts-node -P ./src/test/tsconfig.json ./src/test/test-all",
    "test": "ts-node -P ./src/test/tsconfig.json",
    "check": "  tsc --noEmit --diagnostics",
    "check2": "node --max-old-space-size=8192 ./node_modules/typescript/lib/tsc.js --noEmit --noErrorTruncation --diagnostics",
    "build": "(rm -r ./dist || true) && tsc -d --diagnostics"
  },
  "author": "anyhowstep",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnyhowStep/typed-orm"
  },
  "dependencies": {
    "@anyhowstep/type-util": "^1.9.1",
    "enum-util": "^1.5.1",
    "schema-decorator": "^2.10.1",
    "typed-mysql": "^3.0.0"
  },
  "devDependencies": {
    "@anyhowstep/typed-env": "^2.1.0",
    "@types/node": "^10.0.4",
    "@types/tape": "^4.2.32",
    "tape": "^4.9.0",
    "ts-node": "^6.0.3",
    "typescript": "^3.0.1"
  }
}
